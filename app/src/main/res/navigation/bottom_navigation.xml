<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/bottom_navigation"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.ppapb_tugas9_buttonnavigation_zefanyadiegoforlandicco.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/orderFragment"
        android:name="com.example.ppapb_tugas9_buttonnavigation_zefanyadiegoforlandicco.OrderFragment"
        android:label="fragment_order"
        tools:layout="@layout/fragment_order" >
        <action
            android:id="@+id/action_orderFragment_to_pickerFragment"
            app:destination="@id/pickerFragment" />
        <action
            android:id="@+id/action_orderFragment_to_pickerFragment2"
            app:destination="@id/pickerFragment" />
        <action
            android:id="@+id/action_orderFragment_to_pickerFragment3"
            app:destination="@id/pickerFragment" />
    </fragment>
    <fragment
        android:id="@+id/pickerFragment"
        android:name="com.example.ppapb_tugas9_buttonnavigation_zefanyadiegoforlandicco.PickerFragment"
        android:label="fragment_picker"
        tools:layout="@layout/fragment_picker" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.ppapb_tugas9_buttonnavigation_zefanyadiegoforlandicco.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/ticketFragment"
        android:name="com.example.ppapb_tugas9_buttonnavigation_zefanyadiegoforlandicco.TicketFragment"
        android:label="fragment_ticket"
        tools:layout="@layout/fragment_ticket" >
        <action
            android:id="@+id/action_ticketFragment_to_orderFragment"
            app:destination="@id/orderFragment" />
        <action
            android:id="@+id/action_ticketFragment_to_orderFragment2"
            app:destination="@id/orderFragment" />
    </fragment>
</navigation>